   "source": [
    "import pandas as pd\n",
    "\n",
    "x = pd.read_excel('data.xlsx')\n",
    "x = x.iloc[:,1:].T\n",
    "\n",
    "# mean\n",
    "x_mean = x.mean(axis=1)\n",
    "for i in range(x.index.size):\n",
    "    x.iloc[i,:] = x.iloc[i, :] / x_mean[i]"
   ]
  }
